		"@types/react": "^19.2.5",
		"@types/react-dom": "^19.2.3",
		"@types/scheduler": "^0.26.0",
		"@vitejs/plugin-react": "^5.1.1",
		"eslint": "^9.39.1",
		"happy-dom": "^20.0.10",
		"react": "^19.2.0",
		"react-dom": "^19.2.0",
