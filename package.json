		"@stylistic/eslint-plugin": "^2.12.0",
		"@swc/core": "^1.10.1",
		"@swc/jest": "^0.2.37",
		"@testing-library/dom": "^10.4.0",
		"@testing-library/react": "^16.1.0",
		"@thermarthae/eslint-config": "^5.0.0",
