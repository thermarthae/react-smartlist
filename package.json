		"jest-environment-jsdom": "^29.7.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"rollup": "^4.12.0",
		"rollup-plugin-dts": "^6.1.0",
		"rollup-plugin-swc3": "^0.11.0",
		"typescript": "^5.3.3"
	}
}
