{
	"name": "@thermarthae/react-smartlist",
	"version": "3.0.1",
	"license": "MIT",
	"author": "thermarthae <thermarthae@protonmail.com>",
	"type": "module",
	"main": "./dist/index.cjs",
	"types": "./dist/index.d.ts",
	"exports": {
		"./package.json": "./package.json",
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"default": "./dist/index.cjs"
		},
		"./*": {
			"types": "./dist/*.d.ts",
			"import": "./dist/*.js",
			"default": "./dist/*.cjs"
		}
	},
	"sideEffects": false,
	"packageManager": "yarn@4.9.3+sha512.8295ee814f6b253e16f516416481b481a215ed03ef1ae38524d108084872560a9ed75aeb23b91ab64222062ac4149a594150ae538c2a9536fdbcefd4e49b11cc",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/thermarthae/react-smartlist.git"
	},
	"bugs": {
		"url": "https://github.com/thermarthae/react-smartlist/issues"
	},
	"files": [
		"dist"
	],
	"keywords": [
		"react",
		"virtual",
		"virtualized",
		"list",
		"smartlist",
		"infinite"
	],
	"scripts": {
		"build": "rolldown -c rolldown.config.ts",
		"lint": "eslint ./src --cache",
		"test": "vitest"
	},
	"dependencies": {
		"scheduler": "^0.23.0 || ^0.24.0 || ^0.25.0 || ^0.26.0",
		"shallow-equal": "^3.1.0"
	},
	"peerDependencies": {
		"@types/react": "*",
		"react": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0"
	},
	"peerDependenciesMeta": {
		"@types/react": {
			"optional": true
		}
	},
	"devDependencies": {
		"@eslint/js": "^9.34.0",
		"@stylistic/eslint-plugin": "^5.2.3",
		"@testing-library/dom": "^10.4.1",
		"@testing-library/react": "^16.3.0",
		"@thermarthae/eslint-config": "^6.0.0",
		"@types/node": "^24.3.0",
		"@types/react": "^19.1.11",
		"@types/react-dom": "^19.1.7",
		"@types/scheduler": "^0.26.0",
		"@vitejs/plugin-react": "^5.0.1",
		"eslint": "^9.34.0",
		"eslint-plugin-jsx-a11y": "^6.10.2",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-react-hooks": "^5.2.0",
		"eslint-plugin-simple-import-sort": "^12.1.1",
		"happy-dom": "^18.0.1",
		"react": "^19.1.1",
		"react-dom": "^19.1.1",
		"rolldown": "1.0.0-beta.34",
		"rolldown-plugin-dts": "^0.15.9",
		"typescript": "^5.9.2",
		"typescript-eslint": "^8.41.0",
		"vite": "npm:rolldown-vite@^7.1.5",
		"vitest": "^3.2.4"
	}
}
