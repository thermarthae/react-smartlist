		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"rollup": "^4.28.1",
		"rollup-plugin-dts": "^6.1.1",
		"rollup-plugin-swc3": "^0.12.1",
		"typescript": "^5.7.2",
		"typescript-eslint": "^8.18.0"
	}
