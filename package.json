		"type": "git",
		"url": "git+https://github.com/thermarthae/react-smartlist.git"
	},
	"bugs": {
		"url": "https://github.com/thermarthae/react-smartlist/issues"
	},
	"files": [
		"dist"
	],
	"keywords": [
		"react",
		"virtual",
		"virtualized",
		"list",
		"smartlist",
		"infinite"
	],
	"scripts": {
		"build": "rollup -c",
		"lint": "eslint ./src --cache",
		"test": "jest"
	},
	"dependencies": {
		"scheduler": "^0.23.0 || ^0.25.0",
