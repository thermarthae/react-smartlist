	"type": "module",
	"main": "./dist/index.cjs",
	"types": "./dist/index.d.ts",
	"exports": {
		"./package.json": "./package.json",
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"default": "./dist/index.cjs"
		},
		"./*": {
			"types": "./dist/*.d.ts",
			"import": "./dist/*.js",
			"default": "./dist/*.cjs"
		}
	},
	"sideEffects": false,
	"packageManager": "yarn@4.1.0+sha256.81a00df816059803e6b5148acf03ce313cad36b7f6e5af6efa040a15981a6ffb",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/thermarthae/react-smartlist.git"
	},
	"bugs": {
		"url": "https://github.com/thermarthae/react-smartlist/issues"
	},
	"files": [
		"dist"
	],
	"keywords": [
		"react",
		"virtual",
		"virtualized",
		"list",
		"smartlist",
		"infinite"
	],
	"scripts": {
		"build": "rollup -c",
		"lint": "eslint ./src --ext .ts,.tsx --cache",
		"test": "jest"
	},
	"dependencies": {
		"scheduler": "^0.23.0",
		"shallow-equal": "^3.1.0"
	},
	"peerDependencies": {
		"@types/react": "*",
		"react": "^16.8.0 || ^17.0.0 || ^18.0.0"
